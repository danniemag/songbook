<!DOCTYPE html>
<html>
  <head>
    <title>Songbook</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'https://use.fontawesome.com/releases/v5.0.13/css/all.css', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'dashboard', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'dripicon', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'foundation', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'pace-theme-flash', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'palette', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'slicknav', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'style', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'theme', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'themify-icons', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'typicons', media: 'all', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'pace', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'modernizr', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'waypoints.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'preloader-script', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'foundation', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.slimscroll', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.slicknav', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'sliding-menu', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.counterup.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.circliful', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'app', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'loader', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.flot.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.flot.resize.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery.flot.pie.min', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="pace-done" data-whatinput="mouse" style="overflow: visible;">
    <%= render 'partials/preloader' %>
    <div class="off-canvas-wrapper over-flow-hidden">
      <div class="off-canvas-wrapper-inner" data-off-canvas-wrapper="">
        <div class="inner-wrap">
          <%= render 'partials/sidebar' %>
          <div class="wrap-fluid" id="paper-bg" style="width: auto; margin-left: 260px;">
            <%= render 'partials/topnav' %>
            <%= yield %>
            <%= render 'partials/footer' %>
          </div>
        </div>
        <!-- end paper bg -->
      </div>
    </div>


<!--    <p class="notice"><%#= notice %></p>-->
<!--    <p class="alert"><%#= alert %></p>-->


    <!-- main javascript library -->
    <script type="text/javascript" src="./FondamixHome_files/jquery.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/waypoints.min.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/preloader-script.js"></script>
    <!-- foundation javascript -->
    <script src="./FondamixHome_files/what-input.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/foundation.js"></script>
    <!-- main edumix javascript -->
    <script type="text/javascript" src="./FondamixHome_files/jquery.slimscroll.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/jquery.slicknav.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/sliding-menu.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/scriptbreaker-multiple-accordion-1.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/jquery.counterup.min.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/jquery.circliful.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/app.js"></script>
    <!-- additional javascript -->
    <script type="text/javascript" src="./FondamixHome_files/jquery.velocity.min.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/number-pb.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/loader.js"></script>
    <script type="text/javascript" src="./FondamixHome_files/demo.js"></script>
    <!-- FLOT CHARTS -->
    <script src="./FondamixHome_files/jquery.flot.js" type="text/javascript"></script>
    <!-- FLOT RESIZE PLUGIN - allows the chart to redraw when the window is resized -->
    <script src="./FondamixHome_files/jquery.flot.resize.min.js" type="text/javascript"></script>
    <!-- FLOT PIE PLUGIN - also used to draw donut charts -->
    <script src="./FondamixHome_files/jquery.flot.pie.min.js" type="text/javascript"></script>
    <!-- FLOT CATEGORIES PLUGIN - Used to draw bar charts -->
    <script src="./FondamixHome_files/jquery.flot.categories.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="./FondamixHome_files/skycons.js"></script>
    <script src="./FondamixHome_files/js"></script>

    <script>
        $(document).foundation();
    </script>

    <div class="tooltip-inner" id="line-chart-tooltip"
         style="position: absolute; background: rgb(51, 51, 51); padding: 3px 10px;
             color: rgb(255, 255, 255); display: none; opacity: 1;">
    </div>

  </body>
</html>
